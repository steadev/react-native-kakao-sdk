{
  "name": "@steadev/react-native-kakao-sdk",
  "version": "1.0.0",
  "description": "React Native Module for Kakao SDK (forked from @react-native-seoul/kakao-login@4.2.3)",
  "main": "index.js",
  "types": "index.d.ts",
  "postinstall": "dooboolab-welcome postinstall",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc && flowgen index.d.ts -o index.js.flow",
    "flow": "flow",
    "lint": "eslint -c .eslintrc.js 'index.ts'",
    "validate": "npm run lint && npm run test:typescript && npm run test:flow && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/steadev/react-native-kakao-sdk"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react-native",
    "kakao",
    "kakao-sdk",
    "dooboolab"
  ],
  "author": "dooboolab",
  "license": "MIT",
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "dependencies": {
    "dooboolab-welcome": "^1.3.2"
  },
  "devDependencies": {
    "@dooboo/eslint-config": "^1.3.4",
    "@types/eslint": "^8.4.2",
    "@types/react": "18.0.9",
    "@types/react-native": "^0.67.7",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.16.0",
    "flow-bin": "^0.178.1",
    "flowgen": "^1.19.0",
    "prettier": "^2.6.2",
    "typescript": "^4.6.4"
  }
}
